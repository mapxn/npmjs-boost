{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "https://registry.npmjs.org/$1"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "s-maxage=0"
                }
            ]
        }
    ]
}